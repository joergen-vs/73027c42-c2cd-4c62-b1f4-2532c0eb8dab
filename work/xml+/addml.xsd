					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maxLength" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notNull" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
