	<xs:group name="fieldDefinitionProperties">
		<xs:sequence>
		</xs:sequence>
	</xs:group>

	<xs:group name="fieldDefinitionObjects">
		<xs:sequence>
			<xs:group ref="addml:referenceGroup" minOccurs="0" />
		</xs:sequence>
	</xs:group>

	<xs:complexType name="attributeDefinition">
		<xs:complexContent>
			<xs:extension base="addml:_definition">
				<xs:sequence>
					<xs:group ref="addml:attributeDefinitionProperties" />
					<xs:group ref="addml:attributeDefinitionObjects"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="attributeDefinitionProperties">
