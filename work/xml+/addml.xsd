				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="xmlFieldObjects">
		<xs:sequence>
			<xs:element name="attribute" type="addml:xmlAttribute" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="addml:codesGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>

	<xs:group name="codesGroup">
		<xs:sequence>
			<xs:element name="codes">
				<xs:complexType>
					<xs:sequence>
						<xs:group ref="addml:unitGroup" />
						<xs:element name="codeValue" type="xs:string" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Coded value in a field</xs:documentation>
								<xs:documentation>EX. 0, 1 or A, B, C</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attributeGroup ref="addml:unitAttributeGroup" />
					<xs:attribute name="definitionReference" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>Reference to definition-object which contains the description of the file.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="code">
		<xs:sequence>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="attribute">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
			<xs:group ref="addml:attributeProperties" minOccurs="0" />
			<xs:group ref="addml:additionalProperties" minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />

		<xs:attribute name="definitionReference" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Reference to definition-object which contains the description of the file.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="typeReference" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Reference to types-object which contains how the file is read.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="xmlAttribute">
		<xs:complexContent>
			<xs:extension base="addml:attribute">
				<xs:sequence>
					<xs:group ref="addml:xmlAttributeProperties" />
					<xs:group ref="addml:xmlAttributeObjects" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="attributeProperties">
		<xs:sequence></xs:sequence>
	</xs:group>

	<xs:group name="attributeObjects">
		<xs:sequence>
			<xs:group ref="addml:codesGroup" minOccurs="0" />
		</xs:sequence>
	</xs:group>

	<xs:group name="xmlAttributeProperties">
		<xs:sequence>
		</xs:sequence>
	</xs:group>

	<xs:group name="xmlAttributeObjects">
		<xs:sequence>

			<xs:group ref="addml:codesGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="_folder">
		<xs:sequence>
			<xs:group ref="addml:unitGroup"/>
			<xs:group ref="addml:folderProperties"></xs:group>
			<xs:group ref="addml:additionalProperties" minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<xs:complexType name="folder">
		<xs:complexContent>
			<xs:extension base="addml:_folder">
				<xs:sequence>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="folderProperties">
		<xs:sequence>
			<xs:element name="name" type="addml:name">
				<xs:annotation>
					<xs:documentation>Path to folder relative to this file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="content">
				<xs:annotation>
					<xs:documentation>Type of content in folder.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="DATAFILES" />
						<xs:enumeration value="DOCUMENTATION" />
						<xs:enumeration value="DOCUMENTS" />
						<xs:enumeration value="REPORTS" />
						<xs:enumeration value="OTHER" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="numberOfOccurrences">
				<xs:annotation>
					<xs:documentation>Number of files recursively in folder, based on extension. All files available with filepath equal to '.*'.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="filepath" type="xs:string" />
						<xs:element name="value" type="addml:value">
							<xs:annotation>
								<xs:documentation></xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<!-- Definitions -->

	<xs:complexType name="definitions">
		<xs:sequence>
			<xs:element name="definition" type="addml:_definition" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="_definition">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
			<xs:group ref="addml:versioningProperties" />
			<xs:group ref="addml:catalogDefinitionProperties" minOccurs="0"/>
			<xs:group ref="addml:additionalProperties" minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<xs:group name="versioningProperties">
		<xs:sequence>
			<xs:element name="version" type="addml:version" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Connected to variantOf, displaying a specific iteration of a definition</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>When re-using definitions, explain how the current is connected to referenced</xs:documentation>
				</xs:annotation>
				<xs:element name="subTypeOf" type="xs:string">
					<xs:annotation>
						<xs:documentation>Reference to supertype of definition</xs:documentation>
						<xs:documentation>Ex: (a, b) is supertype of (a, b, c) [Extra field/group]</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="superTypeOf" type="xs:string">
					<xs:annotation>
						<xs:documentation>Reference to subtype of definition</xs:documentation>
						<xs:documentation>Ex: (a, b) is subtype of (a, b, c*)  [Missing field/group]</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="variantOf" type="xs:string">
					<xs:annotation>
						<xs:documentation>Reference to variant of definition</xs:documentation>
						<xs:documentation>Ex: (a, b, c) is variant of (a, b, c*) [Different restraints on same field/group]</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:group>

	<xs:group name="catalogDefinitionProperties">
		<xs:sequence>
			<xs:element name="specification" type="xs:string">
				<xs:annotation>
					<xs:documentation>Reference to specification in reference/content</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="source" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="containerDefinition">
		<xs:complexContent>
			<xs:extension base="addml:_definition">
				<xs:sequence>
					<xs:group ref="addml:containerDefinitionProperties" />
					<xs:group ref="addml:containerDefinitionObjects"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="containerDefinitionProperties">
		<xs:sequence>
			<xs:element name="incomplete" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shows if a object is described in full or partially</xs:documentation>
				</xs:annotation>
				<xs:complexType />
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="containerDefinitionObjects">
		<xs:sequence>
			<xs:group ref="addml:referenceGroup" minOccurs="0" />
		</xs:sequence>
	</xs:group>

	<xs:group name="referenceGroup">
		<xs:sequence>
			<xs:element name="reference" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="value" type="xs:string"/>
					<xs:attribute name="min" use="required">
						<xs:annotation>
							<xs:documentation>Minimum, from 0 up</xs:documentation>
							<xs:documentation>0 = null-able</xs:documentation>
							<xs:documentation>1 = Default</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:nonNegativeInteger"/>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="max" use="required">
						<xs:annotation>
							<xs:documentation>Maximum, from 1 up, included 'unbounded'</xs:documentation>
							<xs:documentation>1 = Default</xs:documentation>
							<xs:documentation>'unbounded' = No specific limit / '1 or more'</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:union>
								<xs:simpleType>
									<xs:restriction base="xs:nonNegativeInteger"/>
								</xs:simpleType>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="unbounded"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:union>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="fileDefinition">
		<xs:complexContent>
			<xs:extension base="addml:_definition">
				<xs:sequence>
					<xs:group ref="addml:fileDefinitionProperties" />
					<xs:group ref="addml:fileDefinitionObjects"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="fileDefinitionProperties">
		<xs:sequence>
			<xs:element name="incomplete" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shows if a object is described in full or partially</xs:documentation>
				</xs:annotation>
				<xs:complexType />
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="fileDefinitionObjects">
		<xs:sequence>
			<xs:group ref="addml:referenceGroup" minOccurs="0" />
		</xs:sequence>
	</xs:group>

	<xs:complexType name="groupDefinition">
		<xs:complexContent>
			<xs:extension base="addml:_definition">
				<xs:sequence>
					<xs:group ref="addml:groupDefinitionProperties" />
					<xs:group ref="addml:groupDefinitionObjects"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="groupDefinitionProperties">
		<xs:sequence>
			<xs:element name="incomplete" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shows if a object is described in full or partially</xs:documentation>
				</xs:annotation>
				<xs:complexType />
			</xs:element>
			<xs:element name="isRecord" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Declares if group is a record</xs:documentation>
				</xs:annotation>
				<xs:complexType/>
			</xs:element>
			<xs:element name="keys" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Primary-, candidate- and foreign-keys associated with groups in file</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:group ref="addml:keyGroup" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="keyGroup">
		<xs:annotation>
			<xs:documentation>Defining relations between groups</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="key">
				<xs:complexType>
					<xs:sequence>
						<xs:choice>
							<xs:element name="primaryKey">
								<xs:complexType/>
							</xs:element>
							<xs:element name="alternateKey">
								<xs:complexType/>
							</xs:element>
							<xs:element name="foreignKey">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="fileReference" type="xs:string" minOccurs="0"/>
										<xs:element name="groupReference" type="xs:string"/>
										<xs:element name="fieldReference" type="xs:string" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Field-reference</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="relationType">
											<xs:annotation>
												<xs:documentation>Relationship between groups</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="1:1">
														<xs:annotation>
															<xs:documentation>One-to-one</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="1:N">
														<xs:annotation>
															<xs:documentation>One-to-many</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="N:1">
														<xs:annotation>
															<xs:documentation>Many-to-one</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="N:M">
														<xs:annotation>
															<xs:documentation>Many-to-many</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:choice>
						<xs:element name="fieldReference" type="xs:string" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Field-reference</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="name" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="groupDefinitionObjects">
		<xs:sequence>
			<xs:group ref="addml:referenceGroup" minOccurs="0" />
		</xs:sequence>
	</xs:group>

