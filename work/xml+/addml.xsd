					<xs:documentation>Obligatory when file contains multiple groups</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fieldIndex" type="xs:nonNegativeInteger" maxOccurs="unbounded">
							<xs:annotation>
