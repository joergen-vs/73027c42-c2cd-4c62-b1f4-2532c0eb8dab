					<xs:sequence>
						<xs:element name="name" type="xs:string" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="charset" type="addml:charset" />
			<xs:element name="charDefinitions" type="addml:charDefinitions" minOccurs="0"/>
		</xs:sequence>
