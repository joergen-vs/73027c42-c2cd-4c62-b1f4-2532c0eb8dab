	<xs:simpleType name="recordSeparator">
		<xs:restriction base="xs:string">
			<xs:enumeration value="CR">
				<xs:annotation>
					<xs:documentation>Carriage Return</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LF">
				<xs:annotation>
