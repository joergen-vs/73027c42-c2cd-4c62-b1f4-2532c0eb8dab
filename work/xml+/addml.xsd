<?xml version="1.0" encoding="utf-8" standalone="no"?>
<xs:schema
		xmlns:addml="http://www.arkivverket.no/standarder/addml/v9"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.arkivverket.no/standarder/addml/v9"
		elementFormDefault="qualified" version="9.0">
	<xs:annotation>
		<xs:documentation xml:lang="en">
			Changes made in versions up to 8.2 are not documented in this document.
			Updated 2014-08-15 and 2014-09-29, Terje Pettersen-Dahl:
			Version 8.3:
			1. Element reference in dataset made optional.
			2. Optional possibility for header-lines.
			3. FieldDefinitionReference made unique within an instance.
			4. Created this documentation section.
		</xs:documentation>
		<xs:documentation xml:lang="en">
			Version 9.0:
			1. Changed all elements to complex- or simpletype, except root-element
			2. Changed structure from specialised to object-oriented, where new objects may be implemented on abstract types
			3. Clarification: All indices are 1-based
			4. Locked down certain properties for objects, while allowing flexibility with 'additionalInfo'
		</xs:documentation>
	</xs:annotation>
	<xs:element name="addml">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addml:dataset" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" />
		</xs:complexType>

		<!-- Logistics -->

		<!-- File-keys -->
		<xs:key name="fileKey">
			<xs:selector xpath=".//addml:file"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="groupKey">
			<xs:selector xpath=".//addml:group"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="recordKey">
			<xs:selector xpath=".//addml:record"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="fieldKey">
			<xs:selector xpath=".//addml:field"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="attributeKey">
			<xs:selector xpath=".//addml:attribute"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="codeKey">
			<xs:selector xpath=".//addml:code"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Definition-keys -->
		<xs:key name="fileDefinitionKey">
			<xs:selector xpath=".//addml:fileDefinition"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="groupDefinitionKey">
			<xs:selector xpath=".//addml:groupDefinition"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="recordDefinitionKey">
			<xs:selector xpath=".//addml:recordDefinition"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="fieldDefinitionKey">
			<xs:selector xpath=".//addml:fieldDefinition"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="attributeDefinitionKey">
			<xs:selector xpath=".//addml:attributeDefinition"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="codeDefinitionKey">
			<xs:selector xpath=".//addml:codeDefinition"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Definition-references -->
		<xs:keyref name="file2DefinitionReference" refer="addml:fileDefinitionKey">
			<xs:selector xpath=".//addml:file" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="group2DefinitionReference" refer="addml:groupDefinitionKey">
			<xs:selector xpath=".//addml:group" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="record2DefinitionReference" refer="addml:recordDefinitionKey">
			<xs:selector xpath=".//addml:record" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="field2DefinitionReference" refer="addml:fieldDefinitionKey">
			<xs:selector xpath=".//addml:field" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="attribute2DefinitionReference" refer="addml:attributeDefinitionKey">
			<xs:selector xpath=".//addml:attribute" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="code2DefinitionReference" refer="addml:codeDefinitionKey">
			<xs:selector xpath=".//addml:code" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>

		<!-- Type-keys -->
		<xs:key name="fileTypeKey">
			<xs:selector xpath=".//addml:fileType"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="recordTypeKey">
			<xs:selector xpath=".//addml:recordType"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="fieldTypeKey">
			<xs:selector xpath=".//addml:fieldType"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="attributeTypeKey">
			<xs:selector xpath=".//addml:attributeType"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Type-references -->
		<xs:keyref name="fileTypeReference" refer="addml:fileTypeKey">
			<xs:selector xpath=".//addml:file" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>
		<xs:keyref name="recordTypeReference" refer="addml:recordTypeKey">
			<xs:selector xpath=".//addml:record" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>
		<xs:keyref name="fieldTypeReference" refer="addml:fieldTypeKey">
			<xs:selector xpath=".//addml:field" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>
		<xs:keyref name="attributeTypeReference" refer="addml:attributeTypeKey">
			<xs:selector xpath=".//addml:attribute" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>

		<!-- /Logistics -->

	</xs:element>
	<xs:element name="dataset">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="description" type="addml:description" minOccurs="0" />
				<xs:element ref="addml:reference" minOccurs="0" />
				<xs:element ref="addml:dataObjects" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="reference">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addml:context" minOccurs="0" />
				<xs:element ref="addml:content" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="context">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="description" type="addml:description" minOccurs="0" />
				<xs:element name="additionalElements" type="addml:additionalElements" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="content">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="description" type="addml:description" minOccurs="0" />
				<xs:element name="additionalElements" type="addml:additionalElements" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="additionalElements">
		<xs:sequence>
			<xs:element name="description" type="addml:description" minOccurs="0" />
			<xs:element name="additionalElement" type="addml:additionalElement" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="additionalElement">
		<xs:sequence>
			<xs:element name="value" type="xs:string" minOccurs="0" />
			<xs:element name="properties" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="property" type="addml:property" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="additionalElements" type="addml:additionalElements" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
		<xs:attribute name="format" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="property">
		<xs:sequence>
			<xs:element name="value" type="xs:string" minOccurs="0" />
			<xs:element name="property" type="addml:property" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
		<xs:attribute name="format" type="xs:string" />
	</xs:complexType>


	<xs:element name="dataObjects">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="container" type="addml:container" maxOccurs="unbounded" />
				<xs:element name="definitions" type="addml:definitions" minOccurs="0" />
				<xs:element name="types" type="addml:types" minOccurs="0" />
				<xs:element name="processes" type="addml:processes" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:group name="unitGroup">
		<xs:sequence>
			<xs:element name="description" type="addml:description" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:simpleType name="description">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<xs:attributeGroup name="unitAttributeGroup">
		<xs:attribute name="name" type="addml:name" use="required">
			<xs:annotation>
				<xs:documentation>Machine-readable name of object</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="label" type="addml:label" use="optional">
			<xs:annotation>
				<xs:documentation>Human-readable name of object</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>

	<xs:simpleType name="name">
		<xs:annotation>
			<xs:documentation>Unique machine- or human-readable name of object</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<xs:simpleType name="label">
		<xs:annotation>
			<xs:documentation>Human-readable name of object</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<!-- Abstract, do not use -->
	<xs:complexType name="container">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<xs:complexType name="archivalExtraction">
		<xs:annotation>
			<xs:documentation>Root-catalog for files and folders making up an archive extraction.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="addml:container">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:containerProperties" />
								<xs:group ref="addml:archiveProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<!-- Files & Folders -->
					<xs:element name="objects">
						<xs:complexType>
							<xs:sequence>
								<xs:choice maxOccurs="unbounded">
									<xs:element name="file" type="addml:_file" />
									<xs:element name="folder" type="addml:_folder" />
								</xs:choice>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="files">
		<xs:annotation>
			<xs:documentation>Root-catalog for files and folders not yet described</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="addml:container">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:containerProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<!-- Files & Folders -->
					<xs:element name="objects">
						<xs:complexType>
							<xs:sequence>
								<xs:choice maxOccurs="unbounded">
									<xs:element name="file" type="addml:_file" />
									<xs:element name="folder" type="addml:_folder" />
								</xs:choice>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="containerProperties">
		<xs:annotation>
			<xs:documentation>Abstract type for all 'packages' of information</xs:documentation>
			<xs:documentation>Archival extraction</xs:documentation>
			<xs:documentation>File structure</xs:documentation>
			<xs:documentation>etc..</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="addml:name" />
			<xs:element name="version" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:group>

	<xs:group name="archiveProperties">
		<xs:annotation>
			<xs:documentation>Archive-properties.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="period">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="startDate">
							<xs:annotation>
								<xs:documentation>Date when the archive or part of is put into active use</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="fieldformat" type="xs:string" default="YYYY-MM-DD" >
											<xs:annotation>
												<xs:documentation>By standard, dates is written as specified in NS-ISO 8601</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="inboundPeriod" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicate whether information created before start-date is included</xs:documentation>
								<xs:documentation>Not relevant for new implementation of systems</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="soft"></xs:enumeration>
									<xs:enumeration value="sharp"></xs:enumeration>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="endDate">
							<xs:annotation>
								<xs:documentation>Date when the archive or part of is taken out of active use</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="fieldformat" type="xs:string" default="YYYY-MM-DD" >
											<xs:annotation>
												<xs:documentation>By standard, dates is written as specified in NS-ISO 8601</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="outgoingPeriod" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicate whether information created after end-date is included</xs:documentation>
								<xs:documentation>Not relevant for end-of-life systems</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="soft"></xs:enumeration>
									<xs:enumeration value="sharp"></xs:enumeration>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="containsScreenedInformation" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True or false: Are there metadata in the archive which is screened?</xs:documentation>
					<xs:documentation>Relevant for archives implementing screening</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="containsDocumentsWhichIsDisposed" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True or False: Are there documents in the archive which is disposed?</xs:documentation>
					<xs:documentation>Relevant for archives implementing disposal</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="containsDocumentsScheduledForDisposal" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True or False: Are there documents in the archive which is scheduled for disposal?</xs:documentation>
					<xs:documentation>Relevant for archives implementing disposal</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:group name="additionalProperties">
		<xs:annotation>
			<xs:documentation>Any additional information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="additionalInfo" type="addml:property"/>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="_file">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<xs:complexType name="file">
		<xs:annotation>
			<xs:documentation>A basic file</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="addml:_file">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:fileProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="dataFile">
		<xs:annotation>
			<xs:documentation>A file containing readable data [not yet described] or metadata.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="addml:_file">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:fileProperties" />
								<xs:group ref="addml:completionProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attributeGroup ref="addml:referenceProperties" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="fixedFile">
		<xs:complexContent>
			<xs:extension base="addml:_file">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:fileProperties" />
								<xs:group ref="addml:completionProperties" />
								<xs:group ref="addml:fixedFileProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="objects" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:fixedFileObjects" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attributeGroup ref="addml:referenceProperties" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="delimitedFile">
		<xs:complexContent>
			<xs:extension base="addml:_file">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:fileProperties" />
								<xs:group ref="addml:completionProperties" />
								<xs:group ref="addml:delimitedFileProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="objects" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:delimitedFileObjects" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attributeGroup ref="addml:referenceProperties" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="xmlFile">
		<xs:complexContent>
			<xs:extension base="addml:_file">
				<xs:sequence>
					<xs:element name="properties">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:fileProperties" />
								<xs:group ref="addml:completionProperties" />
								<xs:group ref="addml:xmlFileProperties" />
								<xs:group ref="addml:additionalProperties" minOccurs="0" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="objects" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:group ref="addml:xmlFileObjects" />
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attributeGroup ref="addml:referenceProperties" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="fileProperties">
		<xs:annotation>
			<xs:documentation>Properties applicable to all files.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="path" type="xs:string">
				<xs:annotation>
					<xs:documentation>Path, to file relative to this file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="checksum">
				<xs:annotation>
					<xs:documentation>Contains the algorithm and value of checksum-calculation for file.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="algorithm">
							<xs:annotation>
								<xs:documentation>Checksum-algorithm used for calculation.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="SHA-256"></xs:enumeration>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="value">
							<xs:annotation>
								<xs:documentation>Calculated hash-value of file.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="addml:value">
									<xs:pattern value="[0-9A-F]{64}" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>

	<xs:attributeGroup name="referenceProperties">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
