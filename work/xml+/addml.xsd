					</xs:key>
					<xs:key name="attributeKey">
						<xs:selector xpath=".//addml:attribute"/>
						<xs:field xpath="@name"/>
					</xs:key>
					<xs:key name="codeKey">
						<xs:selector xpath=".//addml:codes"/>
						<xs:field xpath="@name"/>
