<?xml version="1.0" encoding="utf-8" standalone="no"?>
<xs:schema
		xmlns:addml="http://www.arkivverket.no/standarder/addml/v9"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.arkivverket.no/standarder/addml/v9"
		elementFormDefault="qualified" version="9.0">
	<xs:annotation>
		<xs:documentation xml:lang="en">
			Changes made in versions up to 8.2 are not documented in this document.
			Updated 2014-08-15 and 2014-09-29, Terje Pettersen-Dahl:
			Version 8.3:
			1. Element reference in dataset made optional.
			2. Optional possibility for header-lines.
			3. FieldDefinitionReference made unique within an instance.
			4. Created this documentation section.
		</xs:documentation>
		<xs:documentation xml:lang="en">
			Version 9.0:
			1. Changed all elements to complex- or simpletype, except root-element
			2. Changed structure from specialised to object-oriented, where new objects may be implemented on abstract types
			3. Clarification: All indices are 1-based
			4. Locked down certain properties for objects, while allowing flexibility with 'additionalInfo'
		</xs:documentation>
	</xs:annotation>
	<xs:element name="addml">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="dataset" type="addml:dataset" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required"/>
		</xs:complexType>

		<!-- Logistics -->

		<!-- Reference-keys -->
		<xs:key name="standardKey">
			<xs:selector xpath=".//content/standard"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Reference-references -->
		<xs:keyref name="definition2StandardReference" refer="addml:standardKey">
			<xs:selector xpath=".//addml:definitions/addml:definition" />
			<xs:field xpath="specification" />
		</xs:keyref>

		<!-- File-keys -->
		<xs:key name="fileKey">
			<xs:selector xpath=".//addml:file"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Definition-keys -->
		<xs:key name="definitionKey">
			<xs:selector xpath=".//addml:definitions/addml:definition"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Definition-references -->
		<xs:keyref name="file2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:file" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="container2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:container" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="group2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:group" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="field2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:field" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="attribute2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:attribute" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="codes2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:codes" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="fileType2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:fileType" />
			<xs:field xpath="@definitionReference" />
		</xs:keyref>
		<xs:keyref name="definition2DefinitionReference" refer="addml:definitionKey">
			<xs:selector xpath=".//addml:definition/addml:definitionReference" />
			<xs:field xpath="value" />
		</xs:keyref>

		<!-- Type-keys -->
		<xs:key name="fileTypeKey">
			<xs:selector xpath=".//addml:fileType"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="groupTypeKey">
			<xs:selector xpath=".//addml:groupType"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="fieldTypeKey">
			<xs:selector xpath=".//addml:fieldType"/>
			<xs:field xpath="@name"/>
		</xs:key>
		<xs:key name="attributeTypeKey">
			<xs:selector xpath=".//addml:attributeType"/>
			<xs:field xpath="@name"/>
		</xs:key>

		<!-- Type-references -->
		<xs:keyref name="fileTypeReference" refer="addml:fileTypeKey">
			<xs:selector xpath=".//addml:file" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>
		<xs:keyref name="groupTypeReference" refer="addml:groupTypeKey">
			<xs:selector xpath=".//addml:group" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>
		<xs:keyref name="fieldTypeReference" refer="addml:fieldTypeKey">
			<xs:selector xpath=".//addml:field" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>
		<xs:keyref name="attributeTypeReference" refer="addml:attributeTypeKey">
			<xs:selector xpath=".//addml:attribute" />
			<xs:field xpath="@typeReference" />
		</xs:keyref>

		<!-- /Logistics -->

	</xs:element>

	<xs:complexType name="dataset">
		<xs:sequence>
			<xs:element name="description" type="addml:description" minOccurs="0" />
			<xs:element name="reference" type="addml:reference" minOccurs="0" />
			<xs:element name="container" type="addml:_container" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Where to find information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="definitions" type="addml:definitions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>How to understand information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="types" type="addml:types" minOccurs="0">
				<xs:annotation>
					<xs:documentation>How to read information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="processes" type="addml:processes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>How to review information</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
	</xs:complexType>

	<xs:complexType name="reference">
		<xs:sequence>
			<xs:element name="context" type="addml:context" minOccurs="0" />
			<xs:element name="content" type="addml:content" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="context">
		<xs:sequence>
			<xs:element name="description" type="addml:description" minOccurs="0" />
			<xs:element name="additionalElements" type="addml:additionalElements" minOccurs="0" />
			<xs:element name="agents" type="addml:agents" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="agents">
		<xs:sequence>
			<xs:group ref="addml:unitGroup"/>
			<xs:element name="agent" type="addml:_agent" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="_agent">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup"/>
	</xs:complexType>

	<xs:complexType name="organization">
		<xs:complexContent>
			<xs:extension base="addml:_agent">
				<xs:sequence>
					<xs:element name="role" type="addml:role">
					</xs:element>
					<xs:element name="otherrole" type="xs:string" minOccurs="0"></xs:element>
					<xs:element name="orgnr" type="xs:string" minOccurs="0"></xs:element>
					<xs:element name="operationalPeriod" type="addml:StartAndEndDates" />
					<xs:element name="contact" type="addml:individual" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="software" type="addml:software" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="system" type="addml:system" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:simpleType name="role">
		<xs:restriction base="xs:string">
			<xs:enumeration value="CREATOR"/>
			<xs:enumeration value="EDITOR"/>
			<xs:enumeration value="ARCHIVIST"/>
			<xs:enumeration value="PRESERVATION"/>
			<xs:enumeration value="DISSEMINATOR"/>
			<xs:enumeration value="CUSTODIAN"/>
			<xs:enumeration value="IPOWNER"/>
			<xs:enumeration value="OTHER"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="individual">
		<xs:complexContent>
			<xs:extension base="addml:_agent">
				<xs:sequence>
					<xs:element name="role" type="addml:role"/>
					<xs:element name="otherrole" type="xs:string" minOccurs="0"></xs:element>
					<xs:element name="software" type="addml:software" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="system" type="addml:system" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="software">
		<xs:complexContent>
			<xs:extension base="addml:_agent">
				<xs:sequence>
					<xs:element name="name" type="xs:string"/>
					<xs:element name="version" type="addml:version"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="version">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="scheme" default="Semantic">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Semantic">
								<xs:annotation>
									<xs:documentation></xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Calendar">
								<xs:annotation>
									<xs:documentation></xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:pattern value=".+">
								<xs:annotation>
									<xs:documentation>Other schemes</xs:documentation>
								</xs:annotation>
							</xs:pattern>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="fieldFormat" type="xs:string">
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="system">
		<xs:complexContent>
			<xs:extension base="addml:_agent">
				<xs:sequence>
					<xs:element name="role" type="xs:string"/>
					<xs:element name="title" type="xs:string"/>
					<xs:element name="version" type="addml:version" minOccurs="0"/>
					<xs:element name="type" type="xs:string"/>
					<xs:element name="operationalPeriod" type="addml:StartAndEndDates" />
					<xs:element name="subjects" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="StartAndEndDates">
		<xs:sequence>
			<xs:element name="startDate">
				<xs:simpleType>
					<xs:union>
						<xs:simpleType>
							<xs:restriction base="xs:date"/>
						</xs:simpleType>
						<xs:simpleType>
							<xs:restriction base="xs:nonNegativeInteger"/>
						</xs:simpleType>
					</xs:union>
				</xs:simpleType>
			</xs:element>
			<xs:element name="endDate" type="xs:date" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="content">
		<xs:sequence>
			<xs:element name="description" type="addml:description" minOccurs="0" />
			<xs:element name="standard" type="addml:standard"
						minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="documentation" type="addml:documentation"
						minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="extraction" type="addml:extraction" minOccurs="0" />
			<xs:element name="additionalElements" type="addml:additionalElements" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="standard">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
			<xs:element name="title" type="xs:string">
				<xs:annotation>
					<xs:documentation>Formal name of standard, specification or profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="addml:version">
				<xs:annotation>
					<xs:documentation>Versioning of standard, specification or profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="source" type="xs:anyURI" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Sources of standard, specification or profile. Either a filepath or url</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<xs:complexType name="documentation">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
			<xs:element name="title" type="xs:string">
				<xs:annotation>
					<xs:documentation>Formal name of standard, specification or profile</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<xs:complexType name="extraction">
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Fagsystem"/>
						<xs:enumeration value="Noark-3"/>
						<xs:enumeration value="Noark-4"/>
						<xs:enumeration value="Noark 5"/>
						<xs:pattern value=".+">
							<xs:annotation>
								<xs:documentation>Non-documented type</xs:documentation>
							</xs:annotation>
						</xs:pattern>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="version" type="addml:version">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
	</xs:complexType>

	<!--
	A place for referenced specifications in reference/content 
	
	specification*
		@name = name used for referencing, like 'ISO-8859-1' or 'Noark 5'
		description = Short description of specification
		p:name: Full name
		p:version? = Handling versioning of specification
		p:source+ = Sources of specification. Either a url or filepath
	-->

	<xs:complexType name="additionalElements">
		<xs:sequence>
			<xs:element name="description" type="addml:description" minOccurs="0" />
			<xs:element name="additionalElement" type="addml:additionalElement" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="additionalElement">
		<xs:sequence>
			<xs:element name="value" type="xs:string" minOccurs="0" />
			<xs:element name="properties" type="addml:properties" minOccurs="0" />
			<xs:element name="additionalElements" type="addml:additionalElements" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
		<xs:attribute name="format" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="properties">
		<xs:sequence>
			<xs:element name="property" type="addml:property" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="property">
		<xs:sequence>
			<xs:choice minOccurs="0">
				<xs:element name="value" type="xs:string" />
				<xs:element name="object" type="xs:anyType" />
			</xs:choice>
			<xs:element name="properties" type="addml:properties" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required" />
		<xs:attribute name="dataType" type="xs:string" />
		<xs:attribute name="format" type="xs:string" />
	</xs:complexType>

	<xs:group name="unitGroup">
		<xs:sequence>
			<xs:element name="description" type="addml:description"
						minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>

	<xs:complexType name="description">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="topic" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:attributeGroup name="unitAttributeGroup">
		<xs:attribute name="name" type="addml:name" use="required">
			<xs:annotation>
				<xs:documentation>Machine-readable name of object</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="label" type="addml:label" use="optional">
			<xs:annotation>
				<xs:documentation>Human-readable name of object</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>

	<xs:simpleType name="name">
		<xs:annotation>
			<xs:documentation>Unique machine- or human-readable name of object</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<xs:simpleType name="label">
		<xs:annotation>
			<xs:documentation>Human-readable name of object</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<!-- Structures -->

	<!-- Abstract, do not use -->
	<xs:complexType name="_container">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="addml:unitGroup" />
			<xs:group ref="addml:containerProperties" />
			<xs:group ref="addml:additionalProperties" minOccurs="0" />
		</xs:sequence>
		<xs:attributeGroup ref="addml:unitAttributeGroup" />
		<xs:attribute name="definitionReference" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="archivalExtraction">
		<xs:annotation>
			<xs:documentation>Root-catalog for files and folders making up an archive extraction.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="addml:_container">
				<xs:sequence>
					<xs:group ref="addml:archiveProperties" />
					<xs:group ref="addml:archivalObjects"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="files">
		<xs:annotation>
			<xs:documentation>File-catalog for files and folders not yet described</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="addml:_container">
				<xs:sequence>
					<xs:group ref="addml:archivalObjects"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:group name="containerProperties">
		<xs:annotation>
			<xs:documentation>Abstract type for all 'packages' of information</xs:documentation>
			<xs:documentation>Archival extraction</xs:documentation>
			<xs:documentation>File structure</xs:documentation>
			<xs:documentation>etc..</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="version" type="addml:version" minOccurs="0" />
		</xs:sequence>
	</xs:group>

	<xs:group name="archiveProperties">
		<xs:annotation>
			<xs:documentation>Archive-properties.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="period">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="startDate">
							<xs:annotation>
								<xs:documentation>Date when the archive or part of is put into active use</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="fieldformat" type="xs:string" default="YYYY-MM-DD" >
											<xs:annotation>
												<xs:documentation>By standard, dates is written as specified in NS-ISO 8601</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="inboundPeriod" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicate whether information created before start-date is included</xs:documentation>
								<xs:documentation>Not relevant for new implementation of systems</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="soft"></xs:enumeration>
									<xs:enumeration value="sharp"></xs:enumeration>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="endDate">
							<xs:annotation>
								<xs:documentation>Date when the archive or part of is taken out of active use</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="fieldformat" type="xs:string" default="YYYY-MM-DD" >
											<xs:annotation>
												<xs:documentation>By standard, dates is written as specified in NS-ISO 8601</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="outgoingPeriod" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicate whether information created after end-date is included</xs:documentation>
								<xs:documentation>Not relevant for end-of-life systems</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="soft"></xs:enumeration>
									<xs:enumeration value="sharp"></xs:enumeration>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="containsScreenedInformation" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True or false: Are there metadata in the archive which is screened?</xs:documentation>
					<xs:documentation>Relevant for archives implementing screening</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="containsDocumentsWhichIsDisposed" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True or False: Are there documents in the archive which is disposed?</xs:documentation>
					<xs:documentation>Relevant for archives implementing disposal</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="containsDocumentsScheduledForDisposal" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True or False: Are there documents in the archive which is scheduled for disposal?</xs:documentation>
					<xs:documentation>Relevant for archives implementing disposal</xs:documentation>
				</xs:annotation>
