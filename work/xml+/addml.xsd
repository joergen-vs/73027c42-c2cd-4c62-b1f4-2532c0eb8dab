	<!-- Types -->

	<xs:complexType name="types">
		<xs:sequence>
			<xs:element name="fileType" type="addml:_fileType" maxOccurs="unbounded" />
			<xs:element name="groupType" type="addml:_groupType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="fieldType" type="addml:_fieldType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="attributeType" type="addml:_fieldType" minOccurs="0" maxOccurs="unbounded" />
