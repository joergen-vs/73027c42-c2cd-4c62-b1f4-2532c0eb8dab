@startuml "database"

package "Database" <<Database>> {
    container <.. system

    package "Tables" <<Rectangle>> {
        system *-- database

        database *-- table
        interface db::key
        
        table *-- db::key
        db::key <.. db::primaryKey
        db::key <.. db::foreignKey
        db::key <.. db::candidateKey

        table *-- column


    }

    package "Views" <<Rectangle>> {
        system *-- views
    }

    package "Users" <<Rectangle>> {
        system *-- user
    }
}

@enduml